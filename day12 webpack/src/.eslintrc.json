{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error",
        "no-irregular-whitespace":"error",
        "for-direction":"error",
        "comma-dangle":"error"
    }
}